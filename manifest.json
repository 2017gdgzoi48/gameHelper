{
  "manifest_version": 2,
  "name": "gameHelper",
  "version": "2.1",
  "description": "开挂他不香吗？",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "helper.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup":"popup.html",
    "default_title": "开挂助手"
  },
  "permissions": [
  	"tabs",
    "activeTab",
    "*://*/*",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "web_accessible_resources": [
  ]
}